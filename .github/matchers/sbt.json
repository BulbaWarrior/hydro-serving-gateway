{
    "problemMatcher": [
        {
            "owner": "sbt",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^.*\\[.*(info|warn|error).*\\]\\s.*([A-Z].*[\\w\\.])....$",
                    "severity": 1,
                    "message": 2
                }
            ]
        }
    ]
}
        
